type Query{
    getAllUsers: [User]
    getUser(id: Int!): User
}

type User{
    id: Int
    name: String
    username: String
    email: String
    phone: String
    website: String
    company: String
    address: Address
    company: Company
}
type Company{
    name: String
    catchPhrase: String
    bs: String
}
type Address{
    street: String
    suite: String
    city: String
    zipcode: String
    geo: Geo
}
type Geo{
    lat: String
    lng: String
}